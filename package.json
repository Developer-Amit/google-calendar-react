{
  "name": "google-calendar-react",
  "version": "1.0.0",
  "Author": "Amit Mishra",
  "description": "Integration of Google Calendar API on your Custom Client side OR React component",
  "main": "./build/GoogleAPICalendar.js",
  "scripts": {
    "build": "tslint --project . && tsc && rm *.test.js",
    "test": "jest",
    "babel": "babel ./GoogleAPICalendar.js -d build"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Developer-Amit/google-calendar-react.git"
  },
  "keywords": [
    "google calendar",
    "google",
    "react",
    "calendar",
    "api",
    "google calendar api",
    "calendar events",
    "google calendar sync",
    "google api",
    "react calendar",
    "react google calendar",
    "google calendar react",
    "angular google calendar",
    "node google calendar api",
    "calendar api",
    "vue google calendar",
    "javascript google calendar"
  ],
  "author": "aymeric beudaert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Developer-Amit/google-calendar-react.git/issues"
  },
  "homepage": "https://github.com/Developer-Amit/google-calendar-react.git#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^25.0.0",
    "linted": "0.0.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@types/node": "^9.6.55"
  }
}
